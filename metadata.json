  "dependencies": [
    { "name": "puppetlabs/apache", "version_requirement": ">= 0.9.0 < 2.0.0" },
    { "name": "puppetlabs/passenger", "version_requirement": ">= 0.0.4 < 0.5.0" },
    { "name": "puppetlabs/stdlib", "version_requirement": ">= 4.5.1 < 5.0.0" },
    { "name": "saz/rsyslog", "version_requirement": ">= 3.4.0 < 4.0.0" }
  ]
