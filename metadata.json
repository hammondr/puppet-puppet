{
  "name": "tskirvin-puppet",
  "version": "1.1.2",
  "author": "Tim Skirvin <tskirvin@fnal.gov>",
  "summary": "manage puppet.conf, puppet runs, and puppet masters",
  "license": "Apache-2.0",
  "source": "https://github.com/tskirvin/puppet-puppet",
  "project_page": "https://github.com/tskirvin/puppet-puppet",
  "issues_url": "https://github.com/tskirvin/puppet-puppet/issues",
  "tags": [ "puppet.conf", "passenger", "webrick", "hiera" ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Scientific",
      "operatingsystemmajrelease": [ "6" ]
